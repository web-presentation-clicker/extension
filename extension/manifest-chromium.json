{
  "manifest_version": 3,
  "name": "Untitled Presentation Clicker Extension",
  "description": "uhhh it does the thing, with the thing",
  "version": "0.0",
  "action": {
    "default_popup": "panel/panel.html",
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "service_worker": "present.js",
    "type": "module"
  },
  "content_scripts": [
	{
		"matches": ["https://docs.google.com/presentation/*"],
		"js": ["content-clicker.js"],
		"run_at": "document_end"
	}
  ]
}

